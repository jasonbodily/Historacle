<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Date</th>
    <th>Location</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.date %></td>
    <td><%= event.location %></td>
    <td><%= event.longitude %></td>
    <td><%= event.latitude %></td>
    <td><%= event.description %></td>
    <td><%= link_to 'Show', library_chronicle_event_path(event) %></td>
    <td><%= link_to 'Edit', edit_library_chronicle_path(event) %></td>
    <td><%= link_to 'Destroy', library_chronicle_event_path(event), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_library_chronicle_event_path(@chronicle)%>
